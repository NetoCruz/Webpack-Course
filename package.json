{
  "name": "webpacklearn",
  "version": "1.0.0",
  "description": "Aprendiendo Webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
