{
  "name": "webpacklearn",
  "version": "1.0.0",
  "description": "Aprendiendo Webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multi-entrypoints/webpack.config.js",
    "build:css": "webpack --config  ./cssloaders/webpack.config.js",
    "build:plugins": "webpack --config  ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config  ./webpackdevserver/webpack.config.js",
    "build:babel": "webpack-dev-server --config  ./babel/webpack.config.js",
    "build:react": "webpack-dev-server --config  ./react/webpack.config.js",
    "build:url-loader": "webpack-dev-server --config  ./url-loader/webpack.config.js",
    "build:prepro": "webpack-dev-server --config  ./prepro/webpack.config.js",
    "build:prevent": "webpack --config  ./prevenduplication/webpack.config.js"
